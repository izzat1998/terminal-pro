{
  "include": ["."],
  "exclude": [
    "**/node_modules",
    "**/__pycache__",
    "**/migrations",
    ".venv",
    "venv"
  ],
  "typeCheckingMode": "basic",
  "pythonVersion": "3.13",
  "pythonPlatform": "Darwin",
  "venvPath": ".",
  "venv": ".venv",
  "reportMissingImports": true,
  "reportMissingTypeStubs": false,
  "reportGeneralTypeIssues": true,
  "djangoSettingsModule": "terminal_app.settings"
}
